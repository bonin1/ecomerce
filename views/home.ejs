<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Striketech</title>
    <link rel="icon" href="/static/image/STRIKETECH-1.png">
    <link rel="stylesheet" href="/static/css/home.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Yanone+Kaffeesatz:wght@500&display=swap" rel="stylesheet">
    <link rel="icon" href="https://www.freepnglogos.com/uploads/google-logo-png/google-logo-png-webinar-optimizing-for-success-google-business-webinar-13.png">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/css/bootstrap.min.css" integrity="sha384-r4NyP46KrjDleawBgD5tp8Y7UzmLA05oM1iAEQ17CSuDqnUK2+k9luXQOfXJCJ4I" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/js/bootstrap.min.js" integrity="sha384-oesi62hOLfzrys4LxRF63OJCXdXDipiYWBnvTl9Y9/TRlw5xlKIEHpNyvvDShgf/" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/js/bootstrap.bundle.min.js"></script> 
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Playfair+Display&family=Roboto:wght@300&display=swap" rel="stylesheet">
    
    
</head>
<body>
    <nav class="imagesa navbar navbar-expand-lg navbar-light">
        <div class="pjesa1">
            <div class="logoja">
                <img class="logo" src="/static/image/STRIKETECH-1.png" alt="">
            </div>
        </div>
        <div class="pjesa2">
            <div class="rcmn">
                <form id="search-form">
                    <input type="text" id="search-input" placeholder="Search..." autocomplete="off">
                    <button class="search-submit" type="submit"><i class="fa fa-search" aria-hidden="true"></i></button>
                </form>
                <div id="recommendations"></div>
            </div>
        </div>
        <div class="button1">
            <button 
            style="background-color: #087EA2; position: relative; right: 10px; "
            class="navbar-toggler" 
            type="button" 
            data-toggle="collapse"
            data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" 
            aria-expanded="false"
            aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
        </div>
        <div class="collapse navbar-collapse navbar-nav w-33 " id="navbarSupportedContent">
            <a  id="a1" class="nav-link" href="/">Home</a>
            <a  id="a1" class="nav-link" href="/register">Register</a>
            <% if (isLoggedIn) { %>
                <form action="/logout" id="logout-form" method="POST">
                    <input type="submit" id="a1" class="nav-link" value="Logout">
                </form>
            <% } else { %>
                <a id="a1" class="nav-link" href="/login">Login</a>
            <% } %>
            <div class="carda">
                <a href="/cart"><a class="cart-color" href="/cart"><i   class="fa-sharp fa-solid fa-cart-shopping"></i></a></i><sup><a href="/cart" style="color: white; text-decoration: none;" class="cart-count"></a></sup></a>
            </div>
        </div>
    </nav>
    
    <div class="slideri">
        <div id="carouselExampleInterval" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner " >
                <div class="carousel-item active" style="height: 22rem;" data-bs-interval="3000">
                    <img src="/static/image/Blue Geometric Technology LinkedIn Banner.png" class="img-carousel"  alt="...">
                </div>
                <div class="carousel-item" style="height: 22rem;" data-bs-interval="3000">
                    <img src="/static/image/Black Friday Email Header .png" class="img-carousel"  alt="...">
                </div>
                <div class="carousel-item" style="height: 22rem;" data-bs-interval="3000">
                    <img src="/static/image/Black and Orange Modern Smartwatch Email Header.png" class="img-carousel"  alt="...">
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">p</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">n</span>
            </button>
        </div>
    </div>



    <div class="main">
        <div class="first-part">
            <div class="categories">
                <h1 class="categori-head">Categories</h1>
                <form class="form1" action="/">
                    <button type="button" class="filter-button" data-value="">All categories</button>
                    <button type="button" class="filter-button" data-value="studiodevices">Studio Devices</button>
                    <button type="button" class="filter-button" data-value="gaming">Gaming</button>
                    <button type="button" class="filter-button" data-value="phone&headphones">Phones & Headphones</button>
                    <button type="button" class="filter-button" data-value="watches&cameras">Watches & Cameras</button>
                    <input type="hidden" name="category" id="selected-category" value="">
                </form>
                <form action="/filter" class="rangat" data-url="/filter" method="POST">
                    <label class="min-output" for="minPrice">Minimum price:</label><br>
                    <input class="min-max" type="range" id="minPrice" name="minPrice" min="0" max="1000" value="<%= minPrice %>" oninput="minPriceOutput.value = '$' + minPrice.value">
                    <output for="minPrice" id="minPriceOutput">$<%= minPrice %></output><br>
                    
                    <label class="max-output" for="maxPrice">Maximum price:</label><br>
                    <input class="min-max" type="range" id="maxPrice" name="maxPrice" min="0" max="1000" value="<%= maxPrice %>" oninput="maxPriceOutput.value = '$' + maxPrice.value">
                    <output for="maxPrice" id="maxPriceOutput">$<%= maxPrice %></output><br>
                    
                    <button class="filteri" type="submit">Search</button>
                </form>
            </div>
            
            <!-- <div class="special">
                <h1>Special</h1>
            </div> -->
        </div>
        <div class="second-part">
            <div class="card-container">
                <% data.forEach(row=>{ %>
                        <div class="card1" data-category="<%= row.kategoria %>">
                            <figure class="card1__thumb">
                                <% if (row.produktimage) { %>
                                    <img src="/image/<%= row.id %>" class="card1__image">
                                <% } else { %>
                                    <img src="default_image.jpg" class="card1__image">
                                <% } %>
                                <figcaption class="card1__caption">
                                <h2 class="card1__title"><%= row.emri_produktit %></h2>
                                <% if (row.sasia_produktit > 0) { %>
                                    <h2 class="card1__snippet">Sasia: <%= row.sasia_produktit %></h2>
                                    <button class="add-to-cart" data-id="<%= row.id %>" data-is-logged-in="<%= isLoggedIn %>">Add to cart</button>
                                    <h2 style="font-family: 'Lilita One';">Cmimi: <span style="color: #18C4B8;"><%=row.cmimi_produktit %>â‚¬</span></h2>
                                    <form class="learnmore-form" action="/produkt/<%= row.id %>" method="get">
                                        <input class="add-to-cart" type="submit" value="Learn more">
                                    </form>
                                <% } else { %>
                                    <p class="card1__button">Out of stock</p>
                                <% } %>
                                </figcaption>
                            </figure>
                        </div>
                <% }) %>
            </div>
        </div>
    </div>
    <%- footer %>

    <div id="progress">
        <span id="progress-value">&#x1F815;</span>
    </div>
    <script src="/static/js/Home.js" defer></script>
</body>
</html>